{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost:8080/SpringSecurityRegistration1/logout.html"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Регистрация."
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Login"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='login-box']/form/table/tbody/tr[1]/td[2]"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "username"
      },
      "text": "Sasha"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "123456"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "submit"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Посмотреть объявления"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Создать новое объявление"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "title"
      },
      "text": "Обучение"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "content"
      },
      "text": "Обучаю вождению на своём автомобиле"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//select[@id='header']//option[4]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Обучение"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Обучение"
      }
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "header"
      },
      "value": "услуги"
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "content"
      },
      "value": "Обучаю вождению на своём автомобиле"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Удалить"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}